
//def buildno = BUILD_NUMBER

pipeline {
    agent any
    tools {
        // Install the Maven version configured as "M3" and add it to the path.
        maven "maven3.6.3"
    }
    
    
    stages {
    stage('Build'){
             steps{
            sh "mvn clean package"
                  }
        }
    stage('scan'){
             steps{
            sh "grype /var/lib/jenkins/workspace/Devsecops-1/target/demo-2.0.0-SNAPSHOT.war > log.xls"
              }
    stage('vulnerablity count'){
             steps{
             sh ./shell.sh
             }
           }
        }
     }
}
